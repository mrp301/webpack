# Ignore bundler config.
webpacker/bundle

# Ignore all logfiles and tempfiles.
webpacker/log/*
webpacker/tmp/*
!webpacker/log/.keep
!webpacker/tmp/.keep

# Ignore pidfiles, but keep the directory.
webpacker/tmp/pids/*
!webpacker/tmp/pids/
!webpacker/tmp/pids/.keep

# Ignore uploaded files in development.
webpacker/storage/*
!webpacker/storage/.keep

webpacker/public/assets
webpacker/byebug_history

# Ignore master key for decrypting credentials and more.
/config/master.key

webpacker/public/packs
webpacker/public/packs-test
webpacker/yarn-error.log
webpacker/yarn-debug.log*
webpacker/yarn-integrity
